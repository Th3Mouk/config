{
  "name": "th3mouk/config",
  "type": "library",
  "description": "PHP configurations loading",
  "keywords": ["Config", "Configurations", "Loading", "Management"],
  "homepage": "https://github.com/th3mouk/config",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Bull",
      "email": "me@gabrielbull.com"
    },
    {
      "name": "Jenner",
      "email": "hypxm@qq.com"
    },
    {
      "name": "Jérémy Marodon",
      "email": "marodon.jeremy@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/container": "^1.0",
    "sandrokeil/interop-config": "^2.0",
    "vlucas/phpdotenv": "^2.0",
    "symfony/yaml": "^2.0||^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "doctrine/orm": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Sinergi\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sinergi\\Config\\Tests\\": [
        "tests/Config/Tests",
        "tests/Config/Tests/_includes"
      ]
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  }
}
